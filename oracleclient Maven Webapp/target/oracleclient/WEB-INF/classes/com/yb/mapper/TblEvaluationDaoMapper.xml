<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.yb.dao.TblEvaluationDao">
	<select id="queryAll" parameterType="Integer" resultType="TblEvaluation">
			select EVALUATION_ID,VERSION,CONTENT,TIME,STATUS,STAR,
			ORG_ID as orgId,CUSTOMER_ID as customerIds,TRADEID from TBL_EVALUATION where EVALUATION_ID>#{id}
			
	</select>
	
	<select id="queryMaxId" resultType="Integer">
  		select max(EVALUATION_ID) from TBL_EVALUATION
  	</select>
</mapper>
