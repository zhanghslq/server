		<select id="queryMaxId" resultType="long">
	  		select max(ID) from BJSHELL.TBL_VIRTUALGOODS
	  	</select>
	  	<select id="queryMinId" resultType="long">
	  		select min(ID) from BJSHELL.TBL_VIRTUALGOODS
	  	</select>
</mapper>
