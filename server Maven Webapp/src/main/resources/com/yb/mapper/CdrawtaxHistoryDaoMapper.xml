<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.yb.dao.CdrawtaxHistoryDao">
  	<insert id="insert" parameterType="List">
  		<foreach collection="list" item="cdrawtaxHistory" index="i" open="Begin" separator=";"  close=";End;"   >
			insert into cdrawtax_history(cdraw_period_id,tax_id,cdrawtax_tot_amount)
			
			values(#{cdrawtaxHistory.cdrawPeriodId},#{cdrawtaxHistory.TaxId},#{cdrawtaxHistory.cdrawtaxTotAmount})
		</foreach>
  	</insert>
</mapper>
