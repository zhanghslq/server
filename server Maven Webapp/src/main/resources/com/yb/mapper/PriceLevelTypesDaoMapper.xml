<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.yb.dao.PriceLevelTypesDao">
  	<insert id="insert" parameterType="List">
			insert into Price_Level_Types(Price_Level,Level_Name,Level_Description)
			values
			<foreach collection="list" item="posTerminal" index="i"  separator="," >
			(#{posTerminal.Price_Level},#{posTerminal.Level_Name},#{posTerminal.Level_Description})
		</foreach>
  	</insert>
</mapper>