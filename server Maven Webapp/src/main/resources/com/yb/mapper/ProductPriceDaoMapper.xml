<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.yb.dao.ProductPriceDao">
  	<insert id="insert" parameterType="List">
  		<foreach collection="list" item="productPrice" index="i" open="Begin" separator=";"  close=";End;"   >
			insert into product_price(price_batch_id,product_id,product_price,product_rrp,
			product_maxretailprice,product_price_status)
			
			values(#{productPrice.priceBatchId},#{productPrice.productId},#{productPrice.productPrice},
			#{productPrice.productRrp},#{productPrice.productMaxretailprice},#{productPrice.productPriceStatus})
		</foreach>
  	</insert>
</mapper>