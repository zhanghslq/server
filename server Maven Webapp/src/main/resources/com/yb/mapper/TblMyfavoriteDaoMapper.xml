<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.yb.dao.TblMyfavoriteDao">
  	<insert id="insert" parameterType="List">
  		<foreach collection="list" item="tblMyfavorite" index="i" open="Begin" separator=";"  close=";End;"   >
			insert into TBL_MYFAVORITE(MYFAVORITE_ID,VERSION,ORGID,CARDUSER_ID)
			
			values(#{tblMyfavorite.myfavoriteId},#{tblMyfavorite.version},#{tblMyfavorite.orgid},#{tblMyfavorite.carduserId})
		</foreach>
  	</insert>
</mapper>
