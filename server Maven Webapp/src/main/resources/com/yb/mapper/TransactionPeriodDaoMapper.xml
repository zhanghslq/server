<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.yb.dao.TransactionPeriodDao">
  	<insert id="insert" parameterType="List">
  		<foreach collection="list" item="transactionPeriod" index="i" open="Begin" separator=";"  close=";End;"   >
			insert into transaction_period(transaction_id,period_id)
			values(#{transactionPeriod.transactionId},#{transactionPeriod.periodId})
		</foreach>
  	</insert>
</mapper>
